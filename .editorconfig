
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
max_line_length = 90

[*.ahk]
charset = utf-8-bom
max_line_length = 99
trim_trailing_whitespace = true

[*.bat]
charset = gbk

[*.cmd]
charset = gbk

[*.help]
charset = utf-8
max_line_length = 123
trim_trailing_whitespace = false
